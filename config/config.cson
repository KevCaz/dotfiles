"*":
  "atom-beautify":
    cpp:
      configPath: "~/.uncrustify.cfg"
    executables:
      autopep8: {}
      beautysh: {}
      rscript:
        path: "/usr/bin/Rscript"
    general:
      _analyticsUserId: "3fc44686-334b-470e-a02a-e79f37adc049"
    r: {}
  "atom-ide-ui":
    "atom-ide-terminal":
      fontFamily: "Droid Sans Mono for powerline bold, courier-new, courier, monospace"
    use: {}
  "autocomplete-bibtex":
    bibtex: [
      "/home/kevcaz/Dropbox/kevcaz.bib"
    ]
  "autocomplete-emojis":
    enableUnicodeEmojis: false
  "autocomplete-font-awesome":
    version: "4.7.0"
  "autocomplete-paths":
    scopes: [
      {
        includeCurrentDirectory: true
        prefixes: [
          "source\\(['\"]"
          "require\\(['\"]"
          "read[^ ]*\\(['\"]"
          "load[^ ]*\\(['\"]"
          "\\]\\(['\"]"
        ]
        projectRelativePath: true
        relative: true
        scopes: [
          "source.r"
        ]
      }
      {
        includeCurrentDirectory: true
        prefixes: [
          "src=['\"]"
          "href=['\"]"
          "\\]\\(['\"]"
        ]
        projectRelativePath: false
        relative: true
        scopes: [
          "text.html.basic"
          "source.gfm"
          "source.r"
        ]
      }
    ]
  "autocomplete-plus": {}
  core:
    customFileTypes:
      "source.ini": [
        ".hgrc"
        ".buckconfig"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "language-hugo"
      "language-markdown"
      "markdown-to-html"
      "random"
      "atom-ide-ui"
      "markdown-preview-enhanced"
      "linter"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-material-ui"
      "atom-material-syntax-dark"
    ]
    warnOnLargeFileLimit: 1
  editor:
    fontFamily: "Fira Code"
    fontSize: 15
    invisibles: {}
    lineHeight: 1.4
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "1a84665e-d7b7-4a0c-9bb3-ad96960f1694"
  "file-icons": {}
  "julia-client":
    consoleOptions: {}
    firstBoot: false
    juliaPath: "/home/kevcaz/Github/Applications/julia/usr/bin/julia"
  linter:
    disabledProviders: [
      "lintr"
      "cpplint"
    ]
  "linter-ui-default":
    panelHeight: 300
    showPanel: true
  "metro-ui":
    themeAccentColor: "#e97a43"
  minimap:
    adjustAbsoluteModeHeight: true
  "platformio-ide-terminal":
    ansiColors:
      normal:
        black: "#042029"
        blue: "#3461a4"
        cyan: "#3de2f0"
        green: "#12ae8e"
        magenta: "#89097c"
        red: "#d94d4d"
        yellow: "#d5cd56"
      zBright:
        brightBlack: "#b1adad"
        brightBlue: "#4481dc"
        brightGreen: "#14deb5"
        brightMagenta: "#cc0db8"
        brightRed: "#f95959"
    style:
      core:
        scrollback: 2000
      defaultPanelHeight: "500px"
      theme: "solarized-dark"
      toggles:
        selectToCopy: false
  "scroll-through-time":
    direction: "down"
    modifierKey: "altKey"
  "spell-check":
    addKnownWords: true
    grammars: [
      "source.asciidoc"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.gfm"
      "source.gfm.rmd"
      "text.md"
      "text.Rmd"
      "text.tex.latex"
      "source.r"
    ]
    locales: [
      "en-US"
      "fr-FR"
    ]
  "split-diff":
    autoDiff: false
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  term3:
    colors:
      background: "#1d1d1d"
      brightBlack: "#6c6c6c"
      normalBlack: "#1d1d1d"
      normalGreen: "#2d6546"
      normalRed: "#ae5c68"
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  wordcount:
    alwaysOn: true
".md.text":
  editor:
    showIndentGuide: true
    softWrapAtPreferredLineLength: true
